    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <Description>Blend</Description>
    <PackageIcon>log7.png</PackageIcon>
    <PackageReleaseNotes>Countk</PackageReleaseNotes>
    <NeutralLanguage>en-GB</NeutralLanguage>
    <AssemblyVersion>1.0.0.1</AssemblyVersion>
    <FileVersion>1.0.0.1</FileVersion>
    <Copyright>Countk</Copyright>
    <ApplicationIcon>afj69-8uxpt-002.ico</ApplicationIcon>
    <UserSecretsId>b2619aa3-bdbf-45eb-ae03-a4f11c8fbd8a</UserSecretsId>
    <ApplicationManifest>app.manifest</ApplicationManifest>
    <AssemblyName>Countk</AssemblyName>
    <RootNamespace>Countk</RootNamespace>
    <StartupObject>WpfApp1.App</StartupObject>
